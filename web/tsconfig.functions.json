{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "./netlify/functions-dist",
    "declaration": false
  },
  "include": ["netlify/functions/**/*.ts"],
  "exclude": ["node_modules"]
}
